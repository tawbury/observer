{
  "dashboard": {
    "title": "Observer System Monitoring Dashboard",
    "timezone": "Asia/Seoul",
    "panels": [
      {
        "type": "stat",
        "title": "Universe Size",
        "datasource": "Prometheus",
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 6,
          "h": 8
        },
        "targets": [
          {
            "expr": "observer_universe_size",
            "legendFormat": "Current Size",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "unit": "short"
        }
      },
      {
        "type": "timeseries",
        "title": "Universe Operations Rate",
        "datasource": "Prometheus",
        "gridPos": {
          "x": 6,
          "y": 0,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "rate(observer_universe_created_total[5m])",
            "legendFormat": "Created/sec",
            "refId": "A"
          },
          {
            "expr": "rate(observer_universe_deleted_total[5m])",
            "legendFormat": "Deleted/sec",
            "refId": "B"
          }
        ]
      },
      {
        "type": "stat",
        "title": "Track A Snapshots",
        "datasource": "Prometheus",
        "gridPos": {
          "x": 0,
          "y": 8,
          "w": 6,
          "h": 8
        },
        "targets": [
          {
            "expr": "observer_track_a_snapshots_total",
            "legendFormat": "Total Snapshots",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value"
        }
      },
      {
        "type": "stat",
        "title": "Track B Slot Utilization",
        "datasource": "Prometheus",
        "gridPos": {
          "x": 6,
          "y": 8,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "observer_track_b_slots_allocated / observer_track_b_slots_total",
            "legendFormat": "Utilization",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "unit": "percentunit"
        }
      },
      {
        "type": "stat",
        "title": "Gaps Detected (Total)",
        "datasource": "Prometheus",
        "gridPos": {
          "x": 0,
          "y": 16,
          "w": 6,
          "h": 8
        },
        "targets": [
          {
            "expr": "observer_gaps_detected_total",
            "legendFormat": "Total Gaps",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        }
      },
      {
        "type": "stat",
        "title": "Rate Limit Tokens Used",
        "datasource": "Prometheus",
        "gridPos": {
          "x": 6,
          "y": 16,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "rate(observer_rate_limit_tokens_total[5m])",
            "legendFormat": "Tokens/sec",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        }
      },
      {
        "type": "stat",
        "title": "API Requests/sec",
        "datasource": "Prometheus",
        "gridPos": {
          "x": 0,
          "y": 24,
          "w": 6,
          "h": 8
        },
        "targets": [
          {
            "expr": "rate(observer_api_requests_total[5m])",
            "legendFormat": "Requests/sec",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        }
      },
      {
        "type": "stat",
        "title": "System Uptime",
        "datasource": "Prometheus",
        "gridPos": {
          "x": 6,
          "y": 24,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "observer_system_uptime_seconds",
            "legendFormat": "Uptime",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "unit": "s"
        }
      }
    ],
    "refresh": "30s",
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    },
    "schemaVersion": 38,
    "version": 1,
    "gnetId": null,
    "uid": "observer-system-monitoring"
  },
  "overwrite": true
}
