# 안전한 템플릿으로 재작성
$safeTemplate = @"
# KIS API 환경변수 템플릿
# 실제 사용 시: cp env.template .env 후 아래 값을 실제 값으로 변경

REAL_APP_KEY=your_kis_app_key_here
REAL_APP_SECRET=your_kis_app_secret_here
REAL_ACCOUNT_NO=12345678-01
PHASE15_SOURCE_MODE=kis
PHASE15_SYMBOL=005930
"@

Set-Content -Path "app\obs_deploy\env.template" -Value $safeTemplate -Encoding UTF8

Write-Host "✅ 안전한 env.template 생성 완료" -ForegroundColor Green